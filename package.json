{
  "name": "json-viewer",
  "version": "1.1.0",
  "description": "Pretty JSON viewer for the terminal",
  "main": "lib/json-viewer.js",
  "scripts": {
    "start": "npm run build",
    "build": "npm run transpile && npm run compress",
    "transpile": "babel src --out-file lib/json-viewer.js",
    "compress": "uglifyjs lib/json-viewer.js -m -c -o lib/json-viewer.js",
    "test": "node test/json-viewer-test.js",
    "test:live": "npm run build && npm pack && npm install -g json-viewer-*.tgz && cat ./bin/json-complete >> ~/.json-autocomplete"
  },
  "keywords": [
    "json",
    "cli",
    "viewer",
    "reader",
    "cat"
  ],
  "author": "mjurczyk",
  "license": "MIT",
  "bin": {
    "json": "./bin/json-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mjurczyk/json-viewer"
  },
  "dependencies": {
    "colors": "^1.1.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "uglify-js": "^2.6.2"
  }
}
