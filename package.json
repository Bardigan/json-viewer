{
  "name": "json-cli",
  "version": "1.0.0",
  "description": "Pretty JSON viewer for the terminal",
  "main": "lib/json-cli.js",
  "scripts": {
    "start": "npm run build",
    "build": "npm run transpile && npm run compress",
    "transpile": "babel src --out-file lib/json-cli.js",
    "compress": "uglifyjs lib/json-cli.js -m -c -o lib/json-cli.js",
    "test": "node test/json-cli-test.js"
  },
  "keywords": [
    "json",
    "cli",
    "viewer",
    "reader",
    "cat"
  ],
  "author": "mjurczyk",
  "license": "MIT",
  "files": "./lib/json-cli.js",
  "bin": {
    "json": "./lib/json-cli.js"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "uglify-js": "^2.6.2"
  }
}
