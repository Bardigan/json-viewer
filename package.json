{
  "name": "json-viewer",
  "version": "1.1.0",
  "description": "Pretty JSON viewer for the terminal",
  "main": "lib/json-viewer.js",
  "scripts": {
    "start": "npm run build",
    "build": "npm run transpile && npm run compress",
    "transpile": "babel src/index.js --out-file lib/json-viewer.js",
    "compress": "uglifyjs lib/json-viewer.js -m -c -o lib/json-viewer.js",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "mocha --compilers js:babel-core/register ./test/**/*.spec.js",
    "test:live": "npm run build && npm pack && npm install -g json-viewer-*.tgz && cat ./bin/json-complete >> ~/.json-autocomplete",
    "prepublish": "npm run lint && npm run test"
  },
  "keywords": [
    "json",
    "cli",
    "viewer",
    "reader",
    "cat"
  ],
  "author": "mjurczyk",
  "license": "MIT",
  "bin": {
    "json": "./bin/json-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mjurczyk/json-viewer"
  },
  "dependencies": {
    "colors": "^1.1.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^8.0.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^4.1.2",
    "eslint": "^4.8.0",
    "mocha": "^4.0.0",
    "uglify-js": "^2.6.2"
  }
}
